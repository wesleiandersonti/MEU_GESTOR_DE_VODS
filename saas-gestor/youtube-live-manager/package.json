{
  "name": "youtube-live-manager",
  "version": "1.0.0",
  "description": "Sistema 24/7 para deteccao e exportacao de canais YouTube Live",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "migrate": "ts-node src/db/migrations/runMigration.ts"
  },
  "keywords": [
    "iptv",
    "youtube",
    "m3u",
    "scanner",
    "mariadb"
  ],
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "mysql2": "^3.14.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
